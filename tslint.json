{
    "extends": "tslint:recommended",
    "rules": {
    "max-line-length": {
        "options": [
            255
        ]
    },
    "comment-format": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "quotemark": false,
    "no-console": false
    }
}